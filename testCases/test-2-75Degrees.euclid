p0 = p(0,0)
lr = s(p0;p(10,0))

# only works if random point is within this cone
ard = pi / 6 * ( 2 + rand)
rrd = 1 + rand
prd = p(rrd*cos(ard),rrd*sin(rrd))

a=5 * pi/12
p1=p(cos(a),sin(a))
l1=l(p0;p1)

initial=lr : p0 : prd
required=l1
maxdepth=3
depthfirst=true
shuffle=true
algorithm=CURVE_BASED
constructor=ADVANCED
priority=NO
curveidentification=LINE_TYPES_DISTINCT
maxsolutions=1
