#
# bisect two random points
#

x1 = rand * 10 - 5
y1 = rand * 10 - 5

x2 = rand * 10 - 5
y2 = rand * 10 - 5

xm = (x1+x2)/2
ym = (y1+y2)/2 

p1 = p( x1, y1)
p2 = p( x2, y2)

m  = p( xm, ym)
n  = p( xm + y1 - y2, ym - x1 + x2 )
bisection = l(m;n)

#problem parameters
initial = p1 : p2
required = bisection
maxDepth = 3
depthFirst = true
shuffle = true
maxSolutions = 3
algorithm = curve_based
priority = coarse
