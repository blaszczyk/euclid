#
# construct an equilateral triangle starting with two base points
#

# numeric constants
sqrt3 = sqrt(3)

# points
p1 = p( -1,     0)
p2 = p(  1,     0)
p3 = p(  0, sqrt3)

#curves
l12 = l(p1 - p2)
l23 = l(p2 - p3)
l31 = l(p3 - p1)

#problem parameters
initialPoints = p1; p2
initialCurves =
requiredPoints =
requiredCurves = l12; l23; l31
maxDepth = 5
