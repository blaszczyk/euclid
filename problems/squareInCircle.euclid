#
# construct a square in a circle
#

o=p(0,0)
p1=p(1,0)
p2=p(0,1)
p3=p(-1,0)
p4=p(0,-1)

c   = c(o; p1)
l12 = l(p1;p2)
l23 = l(p2;p3)
l34 = l(p3;p4)
l41 = l(p4;p1)

initial = o : p1 : c
required = l12 : l23 : l34 : l41
maxdepth = 7
depthfirst = false
maxsolutions = 1
algorithm = curve_based
priority = coarse
